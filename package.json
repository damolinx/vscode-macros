{
  "name": "damolinx-macros",
  "displayName": "Macros for VSCode",
  "description": "Macros is a VSCode extension that allows to execute automation scripts.",
  "version": "0.0.1",
  "publisher": "damolinx",
  "icon": "resources/images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/damolinx/vscode-macros"
  },
  "bugs": {
    "url": "https://github.com/damolinx/vscode-macros/issues"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "macro",
    "macros"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "macros.run",
        "title": "Run",
        "category": "Macros"
      },
      {
        "command": "macros.run.show",
        "title": "Show Running Macros",
        "category": "Macros"
      }
    ],
    "configuration": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.6.0",
    "glob": "^10.4.3",
    "mocha": "^10.6.0",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.6.2"
  }
}